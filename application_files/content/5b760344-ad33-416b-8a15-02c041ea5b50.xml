<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>USR_createTextForTooltipsInTechSkillReport</name>
        <uuid>5b760344-ad33-416b-8a15-02c041ea5b50</uuid>
        <description>takes an array of Skills and an array of associated Skill levels and formats them with the appropriate text for a tooltip.</description>
        <parentUuid>_a-0000dc08-3c7c-8000-9ba2-011c48011c48_220112</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>

index(ri!skills, ri!index, null) &amp; &quot; (&quot; &amp; index(ri!skillLevels, ri!index, null) &amp; &quot;)&quot;
</definition>
        <namedTypedValue>
            <name>skills</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>skillLevels</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>index</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
